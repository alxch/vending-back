{
  "name": "vending-back",
  "version": "0.2.4",
  "private": true,
  "scripts": {
    "start": "node ./www",
    "build": "ncc build www -m -o build",
    "pack": "mkdir -p ../release && cp -rf build/* ../release && mkdir -p ../prebuilds && cp -rf node_modules/@serialport/bindings-cpp/prebuilds/* ../prebuilds",
    "update": "npm install git+ssh://git@github.com:alxch/node-cashcode-bv.git"
  },
  "dependencies": {
    "cashcode-bv": "github:alxch/node-cashcode-bv",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "node-fetch": "^3.3.2",
    "node-localstorage": "^3.0.5",
    "serialport": "^12.0.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.3"
  }
}
